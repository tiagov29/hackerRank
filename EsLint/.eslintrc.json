{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","plugin:node/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars":"warn",
        "no-undef":"off",
        "func-names":"error"
        // "prettier/prettier": "error"
    }
}
